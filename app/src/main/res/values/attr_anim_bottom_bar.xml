<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimBottomBar">
        <attr name="anim_bottom_background" format="color|reference"/>
        <attr name="anim_bottom_selectColor" format="color|reference"/>
        <attr name="anim_bottom_textColor" format="color|reference"/>
        <attr name="anim_bottom_textSelectColor" format="color|reference"/>
        <attr name="anim_bottom_enableAnimation" format="boolean"/>
        <attr name="anim_bottom_animationType">ss
            <enum name="normal" value="0"/>
            <enum name="scale" value="1"/>
            <enum name="gradient" value="2"/>
            <enum name="translate" value="3"/>
        </attr>
    </declare-styleable>
</resources>
