<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CouponView">
        <attr name="coupon_seperaotr_width" format="dimension|reference"/>
        <attr name="coupon_seperator_color" format="color|reference"/>
        <attr name="coupon_border_color" format="color|reference"/>
        <attr name="coupon_binder_radius" format="dimension|reference"/>
        <attr name="coupon_check_radius" format="dimension|reference"/>
        <attr name="coupon_backgroud" format="color"/>
        <attr name="coupon_water_marker" format="string"/>
        <attr name="coupon_marker_color" format="color|reference"/>
        <attr name="coupon_marker_size" format="dimension|reference"/>
        <attr name="coupon_left_color" format="color|reference"/>
        <attr name="coupon_right_color" format="color|reference"/>
        <attr name="coupon_marker_gravity">
            <enum name="upper_left" value="1"/>
            <enum name="lower_left" value="2"/>
            <enum name="upper_right" value="3"/>
            <enum name="lower_right" value="4"/>
            <enum name="center" value="5"/>
        </attr>
        <attr name="coupon_marker_skew_angle" format="float"/>
    </declare-styleable>
</resources>