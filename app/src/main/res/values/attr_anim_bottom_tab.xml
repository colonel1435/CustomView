<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimBottomTab">
        <attr name="anim_tab_text" format="string|reference" />
        <attr name="anim_tab_textSize" format="dimension|reference"/>
        <attr name="anim_tab_textColor" format="color|reference"/>
        <attr name="anim_tab_selectColor" format="color|reference"/>
        <attr name="anim_tab_image" format="reference"/>
        <attr name="anim_tab_selectImage" format="reference"/>
        <attr name="anim_tab_type">
            <enum name="normal" value="0"/>
            <enum name="message" value="1"/>
            <enum name="user" value="2"/>
            <enum name="menu" value="3"/>
        </attr>
    </declare-styleable>
</resources>
