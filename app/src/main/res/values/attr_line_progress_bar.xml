<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineProgressBar">
        <attr name="line_progress_min" format="integer"/>
        <attr name="line_progress_max" format="integer"/>
        <attr name="line_progress_reached_tag" format="string|reference"/>
        <attr name="line_progress_unreached_tag" format="string|reference"/>
        <attr name="line_progress_unreached_color" format="color"/>
        <attr name="line_progress_reached_color" format="color"/>
        <attr name="line_progress_height" format="dimension"/>
        <attr name="line_progress_tag_size" format="dimension"/>
        <attr name="line_progress_tag_background" format="integer|reference"/>
        <attr name="line_progress_tag_color" format="integer|reference"/>
        <attr name="line_progress_text_size" format="dimension"/>
        <attr name="line_progress_text_color" format="color"/>
        <attr name="line_progress_text_offset" format="dimension"/>
        <attr name="line_progress_text_visiable" format="enum">
            <enum name="visiable" value="0"/>
            <enum name="invisiable" value="1"/>
        </attr>
    </declare-styleable>
</resources>